{
    "redirects": [
        {
            "source": "/:path(.+)\\.html",
            "destination": "/:path",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/((?!manifest\\.json|favicon\\.ico|robots\\.txt|img/).*)",
            "destination": "/"
        }
    ]
}